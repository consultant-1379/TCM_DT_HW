     DBTRI;                                                                          
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=960  ,BRNO=30 ,MAGNO=14 ,SLOTNO= 0 ,BUSCONN=YES;  ! RPSCB1E SCB-A         ! 
