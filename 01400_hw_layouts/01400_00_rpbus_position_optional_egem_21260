     DBTSI:TAB=RPSRPBSPOS ,RPADDR=967  ,BRNO=30 ,MAGNO=14 ,SLOTNO= 4 ,BUSCONN=NO ;  ! GARP2E  LDAP SDA-15   !
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=968  ,BRNO=30 ,MAGNO=14 ,SLOTNO= 5 ,BUSCONN=NO ;  ! GARP2E  LDAP SDA-16   !
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=984  ,BRNO=30 ,MAGNO=14 ,SLOTNO=20 ,BUSCONN=NO ;  ! GARP2E  LDAP SDA-17   !
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=985  ,BRNO=30 ,MAGNO=14 ,SLOTNO=21 ,BUSCONN=NO ;  ! GARP2E  LDAP SDA-18   !
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=986  ,BRNO=30 ,MAGNO=14 ,SLOTNO=22 ,BUSCONN=NO ;  ! GARP2E  LDAP SDA-19   !
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=961  ,BRNO=30 ,MAGNO=14 ,SLOTNO=25 ,BUSCONN=YES;  ! RPSCB1E SCB-B         ! 
     DBTRE:COM;                                                                   
     
     
       
     !*********************************************! 
     !***   APT EXTRA GARP IN OPTIONAL MAG      ***! 
