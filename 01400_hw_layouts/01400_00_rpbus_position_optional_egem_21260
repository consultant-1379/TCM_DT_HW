     DBTRI;  
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=970  ,BRNO=30 ,MAGNO=14 ,SLOTNO=7  ,BUSCONN=NO ;  ! GARP2E  AGEN  -48&&-63 !     
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=971  ,BRNO=30 ,MAGNO=14 ,SLOTNO=8  ,BUSCONN=NO ;  ! GARP2E  AGEN2 -48&&-63 !       
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=981  ,BRNO=30 ,MAGNO=14 ,SLOTNO=17 ,BUSCONN=NO ;  ! GARP2E  AGEN  -64&&-79 !     
     DBTSI:TAB=RPSRPBSPOS ,RPADDR=982  ,BRNO=30 ,MAGNO=14 ,SLOTNO=18 ,BUSCONN=NO ;  ! GARP2E  AGEN2 -64&&-79 !       
     DBTRE:COM;      
